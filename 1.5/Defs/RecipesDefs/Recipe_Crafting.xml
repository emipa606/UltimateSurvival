<Defs>
  <RecipeDef>
    <defName>CreatePowerEye</defName>
    <label> Create power bionic eye</label>
    <description>A mechanical eye that improves sight</description>
    <jobString>Crafting Bionic</jobString>
    <workAmount>5000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Purepowercrystal</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>ComponentIndustrial</li>
        <li>Purepowercrystal</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PowerEye>1</PowerEye>
    </products>
    <skillRequirements>
      <Crafting>15</Crafting>
    </skillRequirements>
    <researchPrerequisite>USBionics</researchPrerequisite>
    <recipeUsers>
      <li>TableBionicProduction</li>
    </recipeUsers>
  </RecipeDef>
